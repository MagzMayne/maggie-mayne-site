// ============================================
// STATE MANAGEMENT
// ============================================

let state = {
    currentDomain: 1,
    completed: new Set(),
    responses: {},
    scales: {}
};

// ============================================
// INITIALIZATION
// ============================================

document.addEventListener('DOMContentLoaded', () => {
    initTabs();
    initDomainNav();
    initDomainSections();
    initScaleButtons();
    initGenerateResults();
});

function initTabs() {
    document.querySelectorAll('.main-tab').forEach(tab => {
        tab.addEventListener('click', () => {
            document.querySelectorAll('.main-tab').forEach(t => t.classList.remove('active'));
            document.querySelectorAll('.tab-content').forEach(c => c.classList.remove('active'));
            tab.classList.add('active');
            document.getElementById(tab.dataset.tab).classList.add('active');
        });
    });
}

function switchTab(tabId) {
    document.querySelectorAll('.main-tab').forEach(t => t.classList.remove('active'));
    document.querySelectorAll('.tab-content').forEach(c => c.classList.remove('active'));
    document.querySelector(`[data-tab="${tabId}"]`).classList.add('active');
    document.getElementById(tabId).classList.add('active');
}

function initDomainNav() {
    const nav = document.getElementById('domainNav');
    domains.forEach(d => {
        const btn = document.createElement('button');
        btn.className = `domain-btn ${d.category}`;
        btn.innerHTML = d.emoji;
        btn.title = `Domain ${d.id}: ${d.name}`;
        btn.onclick = () => showDomain(d.id);
        nav.appendChild(btn);
    });
}

function initDomainSections() {
    const container = document.getElementById('domainSections');
    
    domains.forEach(d => {
        const section = document.createElement('div');
        section.className = 'domain-section';
        section.id = `domain-${d.id}`;
        if (d.id === 1) section.classList.add('active');
        
        section.innerHTML = `
            <div class="domain-header">
                <div class="emoji">${d.emoji}</div>
                <h2>Domain ${d.id}: ${d.name}</h2>
                <p class="archetype">${d.archetype}</p>
                <div class="domain-meta">
                    <span class="meta-badge element">${d.element}</span>
                    <span class="meta-badge zodiac">‚ôà ${d.zodiac}</span>
                    <span class="meta-badge chakra">üîÆ ${d.chakra}</span>
                </div>
                <p class="domain-description">${d.description}</p>
            </div>
            
            <div class="tfh-info">
                <h4>‚ö° Touch For Health / Applied Kinesiology</h4>
                <div class="tfh-grid">
                    <div class="tfh-item">
                        <div class="label">Meridian</div>
                        <div class="value">${d.meridian}</div>
                    </div>
                    <div class="tfh-item">
                        <div class="label">Associated Muscle</div>
                        <div class="value">${d.muscle}</div>
                    </div>
                    <div class="tfh-item">
                        <div class="label">Season</div>
                        <div class="value">${d.season}</div>
                    </div>
                    <div class="tfh-item">
                        <div class="label">Mineral/Nutrient</div>
                        <div class="value">${d.mineral}</div>
                    </div>
                </div>
            </div>
            
            ${generateQuestions(d)}
            
            <div class="nav-buttons">
                <button class="nav-btn prev" onclick="prevDomain()" ${d.id === 1 ? 'disabled' : ''}>‚Üê Previous</button>
                <button class="nav-btn next" onclick="nextDomain()">${d.id === 19 ? 'Complete Assessment' : 'Next Domain ‚Üí'}</button>
            </div>
        `;
        
        container.appendChild(section);
    });
}

function generateQuestions(domain) {
    let html = '';
    domain.questions.forEach((q, i) => {
        if (q.type === 'scale') {
            html += `
                <div class="question-group">
                    <label>${q.text}</label>
                    <div class="scale-group">
                        <span class="scale-label">Low</span>
                        <div class="scale-options">
                            ${[1,2,3,4,5].map(v => `<button class="scale-btn" data-domain="${domain.id}" data-q="${i}" data-value="${v}">${v}</button>`).join('')}
                        </div>
                        <span class="scale-label">High</span>
                    </div>
                </div>
            `;
        } else if (q.type === 'checkbox') {
            html += `
                <div class="question-group">
                    <label>${q.text}</label>
                    <div class="checkbox-group">
                        ${q.options.map((opt, j) => `
                            <label class="check-option">
                                <input type="checkbox" data-domain="${domain.id}" data-q="${i}" data-opt="${j}" data-maps="${opt.maps.join(',')}">
                                <span>${opt.text}</span>
                            </label>
                        `).join('')}
                    </div>
                </div>
            `;
        }
    });
    return html;
}

function initScaleButtons() {
    document.addEventListener('click', (e) => {
        if (e.target.classList.contains('scale-btn') && e.target.dataset.domain) {
            const domain = e.target.dataset.domain;
            const q = e.target.dataset.q;
            const value = e.target.dataset.value;
            
            // Remove selected from siblings
            e.target.parentElement.querySelectorAll('.scale-btn').forEach(btn => btn.classList.remove('selected'));
            e.target.classList.add('selected');
            
            // Store response
            if (!state.scales[domain]) state.scales[domain] = {};
            state.scales[domain][q] = parseInt(value);
            
            checkDomainCompletion(parseInt(domain));
        }
    });
    
    // Handle checkbox changes
    document.addEventListener('change', (e) => {
        if (e.target.type === 'checkbox' && e.target.dataset.domain) {
            const domain = parseInt(e.target.dataset.domain);
            checkDomainCompletion(domain);
        }
    });
}

function checkDomainCompletion(domainId) {
    // Simple check: if any question answered, mark as completed
    const section = document.getElementById(`domain-${domainId}`);
    const hasScale = section.querySelector('.scale-btn.selected');
    const hasChecked = section.querySelector('input[type="checkbox"]:checked');
    
    if (hasScale || hasChecked) {
        state.completed.add(domainId);
        document.querySelectorAll('.domain-btn')[domainId - 1].classList.add('completed');
        updateProgress();
    }
}

function updateProgress() {
    const count = state.completed.size;
    document.getElementById('completedCount').textContent = count;
    document.getElementById('progressFill').style.width = `${(count / 19) * 100}%`;
}

function showDomain(id) {
    state.currentDomain = id;
    document.querySelectorAll('.domain-section').forEach(s => s.classList.remove('active'));
    document.querySelectorAll('.domain-btn').forEach(b => b.classList.remove('active'));
    document.getElementById(`domain-${id}`).classList.add('active');
    document.querySelectorAll('.domain-btn')[id - 1].classList.add('active');
}

function prevDomain() {
    if (state.currentDomain > 1) {
        showDomain(state.currentDomain - 1);
        window.scrollTo({ top: 0, behavior: 'smooth' });
    }
}

function nextDomain() {
    if (state.currentDomain < 19) {
        showDomain(state.currentDomain + 1);
        window.scrollTo({ top: 0, behavior: 'smooth' });
    } else {
        generateProtocol();
    }
}

function initGenerateResults() {
    document.getElementById('generateResults').addEventListener('click', generateProtocol);
}

function generateProtocol() {
    // Collect all responses
    const allEssences = new Map();
    const domainScores = [];
    
    // Collect TCM data
    const sixEvils = {
        wind: document.querySelectorAll('[data-evil="wind"]:checked').length,
        cold: document.querySelectorAll('[data-evil="cold"]:checked').length,
        summerheat: document.querySelectorAll('[data-evil="summerheat"]:checked').length,
        damp: document.querySelectorAll('[data-evil="damp"]:checked').length,
        dry: document.querySelectorAll('[data-evil="dry"]:checked').length,
        heat: document.querySelectorAll('[data-evil="heat"]:checked').length
    };
    
    const fiveElements = {
        wood: document.querySelectorAll('[data-element="wood"]:checked').length,
        fire: document.querySelectorAll('[data-element="fire"]:checked').length,
        earth: document.querySelectorAll('[data-element="earth"]:checked').length,
        metal: document.querySelectorAll('[data-element="metal"]:checked').length,
        water: document.querySelectorAll('[data-element="water"]:checked').length
    };
    
    const meridianClock = [];
    document.querySelectorAll('[data-clock]:checked').forEach(cb => {
        meridianClock.push(cb.dataset.clock);
    });
    
    const akMuscles = [];
    document.querySelectorAll('[data-ak="muscle"]:checked').forEach(cb => {
        akMuscles.push({ muscle: cb.parentElement.textContent.trim(), meridian: cb.dataset.meridian });
    });
    
    domains.forEach(d => {
        let score = 0;
        const section = document.getElementById(`domain-${d.id}`);
        
        // Get scale values (lower = more need)
        const scaleBtn = section.querySelector('.scale-btn.selected');
        if (scaleBtn) {
            const val = parseInt(scaleBtn.dataset.value);
            score += (6 - val);
            
            const qIndex = parseInt(scaleBtn.dataset.q);
            const question = d.questions[qIndex];
            if (val <= 2 && question.maps.low) {
                question.maps.low.forEach(e => {
                    allEssences.set(e, (allEssences.get(e) || 0) + 2);
                });
            } else if (val <= 3 && question.maps.medium) {
                question.maps.medium.forEach(e => {
                    allEssences.set(e, (allEssences.get(e) || 0) + 1);
                });
            }
        }
        
        section.querySelectorAll('input[type="checkbox"]:checked').forEach(cb => {
            if (cb.dataset.maps) {
                score += 1;
                const maps = cb.dataset.maps.split(',');
                maps.forEach(e => {
                    if (e) allEssences.set(e, (allEssences.get(e) || 0) + 1);
                });
            }
        });
        
        domainScores.push({ id: d.id, name: d.name, emoji: d.emoji, score, domain: d });
    });
    
    domainScores.sort((a, b) => b.score - a.score);
    const sortedEssences = [...allEssences.entries()].sort((a, b) => b[1] - a[1]);
    const sortedEvils = Object.entries(sixEvils).sort((a, b) => b[1] - a[1]).filter(e => e[1] > 0);
    const sortedElements = Object.entries(fiveElements).sort((a, b) => b[1] - a[1]).filter(e => e[1] > 0);
    
    const evilEmojis = { wind: 'üå¨Ô∏è', cold: '‚ùÑÔ∏è', summerheat: '‚òÄÔ∏è', damp: 'üíß', dry: 'üèúÔ∏è', heat: 'üî•' };
    const evilNames = { wind: 'Wind (Feng)', cold: 'Cold (Han)', summerheat: 'Summer Heat (Shu)', damp: 'Dampness (Shi)', dry: 'Dryness (Zao)', heat: 'Fire/Heat (Huo)' };
    const elementEmojis = { wood: 'üå≤', fire: 'üî•', earth: 'üåç', metal: '‚ö™', water: 'üíß' };
    const elementOrgans = { wood: 'Liver/Gallbladder', fire: 'Heart/Small Intestine', earth: 'Spleen/Stomach', metal: 'Lung/Large Intestine', water: 'Kidney/Bladder' };
    
    const resultsContent = document.getElementById('resultsContent');
    const clientName = document.getElementById('clientName').value || 'Client';
    
    resultsContent.innerHTML = `
        <div class="results-header">
            <h2>‚ú® Soul Architecture‚Ñ¢ Protocol for ${clientName}</h2>
            <p style="color: rgba(255,248,240,0.7);">Generated ${new Date().toLocaleDateString()}</p>
        </div>
        
        ${sortedEvils.length > 0 ? `
        <div style="background: rgba(255,0,0,0.08); border: 1px solid rgba(255,0,0,0.3); border-radius: 12px; padding: 1.5rem; margin-bottom: 1.5rem;">
            <h3 style="color: #ff6b6b; font-family: 'Cinzel', serif; margin-bottom: 1rem;">üëπ Pathogenic Factors (Six Evils)</h3>
            <div style="display: flex; flex-wrap: wrap; gap: 0.75rem;">
                ${sortedEvils.map(([evil, count]) => `
                    <div style="background: rgba(0,0,0,0.2); padding: 0.75rem 1rem; border-radius: 10px; text-align: center;">
                        <div style="font-size: 1.5rem;">${evilEmojis[evil]}</div>
                        <div style="font-weight: 600; color: #ff6b6b;">${evilNames[evil]}</div>
                        <div style="font-size: 0.8rem; color: rgba(255,248,240,0.6);">${count} indicators</div>
                    </div>
                `).join('')}
            </div>
        </div>
        ` : ''}
        
        ${sortedElements.length > 0 ? `
        <div style="background: rgba(0,206,209,0.08); border: 1px solid rgba(0,206,209,0.3); border-radius: 12px; padding: 1.5rem; margin-bottom: 1.5rem;">
            <h3 style="color: var(--teal); font-family: 'Cinzel', serif; margin-bottom: 1rem;">üå≤ Five Element Imbalances</h3>
            <div style="display: flex; flex-wrap: wrap; gap: 0.75rem;">
                ${sortedElements.map(([element, count]) => `
                    <div style="background: rgba(0,0,0,0.2); padding: 0.75rem 1rem; border-radius: 10px; text-align: center;">
                        <div style="font-size: 1.5rem;">${elementEmojis[element]}</div>
                        <div style="font-weight: 600; color: var(--teal);">${element.toUpperCase()}</div>
                        <div style="font-size: 0.85rem; color: var(--cream);">${elementOrgans[element]}</div>
                        <div style="font-size: 0.8rem; color: rgba(255,248,240,0.6);">${count} indicators</div>
                    </div>
                `).join('')}
            </div>
        </div>
        ` : ''}
        
        ${meridianClock.length > 0 ? `
        <div style="background: rgba(153,50,204,0.08); border: 1px solid rgba(153,50,204,0.3); border-radius: 12px; padding: 1.5rem; margin-bottom: 1.5rem;">
            <h3 style="color: var(--purple-light); font-family: 'Cinzel', serif; margin-bottom: 1rem;">üïê Meridian Clock Findings</h3>
            <p style="color: rgba(255,248,240,0.8);">Symptoms/waking at: ${meridianClock.map(m => m.replace('-', ' ').toUpperCase()).join(', ')}</p>
        </div>
        ` : ''}
        
        ${akMuscles.length > 0 ? `
        <div style="background: rgba(255,215,0,0.08); border: 1px solid rgba(255,215,0,0.3); border-radius: 12px; padding: 1.5rem; margin-bottom: 1.5rem;">
            <h3 style="color: var(--gold); font-family: 'Cinzel', serif; margin-bottom: 1rem;">üí™ Applied Kinesiology Muscle-Meridian Focus</h3>
            <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(200px, 1fr)); gap: 0.75rem;">
                ${akMuscles.map(m => `
                    <div style="background: rgba(0,0,0,0.2); padding: 0.75rem; border-radius: 8px;">
                        <div style="font-size: 0.9rem; color: var(--cream);">${m.muscle}</div>
                    </div>
                `).join('')}
            </div>
        </div>
        ` : ''}
        
        <div class="priority-domains">
            <h3>üéØ Priority Domains (Top 5)</h3>
            ${domainScores.slice(0, 5).map((d, i) => `
                <div class="priority-item">
                    <div class="rank">${i + 1}</div>
                    <div class="emoji">${d.emoji}</div>
                    <div class="info">
                        <div class="name">Domain ${d.id}: ${d.name}</div>
                        <div class="score">${d.domain.archetype} ‚Ä¢ ${d.domain.element} ‚Ä¢ ${d.domain.meridian}</div>
                    </div>
                </div>
            `).join('')}
        </div>
        
        <div class="kinesiology-section">
            <h3>‚ö° Touch For Health / Applied Kinesiology Focus</h3>
            <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(200px, 1fr)); gap: 1rem;">
                ${domainScores.slice(0, 5).map(d => `
                    <div style="background: rgba(0,206,209,0.1); border: 1px solid rgba(0,206,209,0.3); border-radius: 10px; padding: 1rem;">
                        <h4 style="color: var(--teal); margin-bottom: 0.5rem;">${d.emoji} ${d.name}</h4>
                        <p style="font-size: 0.85rem;"><strong>Meridian:</strong> ${d.domain.meridian}</p>
                        <p style="font-size: 0.85rem;"><strong>Muscle:</strong> ${d.domain.muscle}</p>
                        <p style="font-size: 0.85rem;"><strong>Mineral:</strong> ${d.domain.mineral}</p>
                        <p style="font-size: 0.85rem;"><strong>Season:</strong> ${d.domain.season}</p>
                    </div>
                `).join('')}
            </div>
        </div>
        
        <div class="essence-section">
            <h3>üå∏ Recommended Flower Essences (Top 12)</h3>
            <div class="essence-grid">
                ${sortedEssences.slice(0, 12).map(([name, count]) => `
                    <div class="essence-card">
                        <div class="name">${name}</div>
                        <div class="source">Frequency: ${count}</div>
                    </div>
                `).join('')}
            </div>
        </div>
        
        <div class="treatment-section">
            <h3>üìã Comprehensive Treatment Plan</h3>
            <div style="background: rgba(255,215,0,0.08); border: 1px solid rgba(255,215,0,0.3); border-radius: 12px; padding: 1.5rem;">
                ${sortedEvils.length > 0 ? `
                <div style="margin-bottom: 1rem; padding: 1rem; background: rgba(255,0,0,0.1); border-radius: 8px;">
                    <strong style="color: #ff6b6b;">TCM Pattern:</strong> 
                    ${sortedEvils.slice(0, 2).map(([evil]) => evilNames[evil]).join(' + ')}
                    ${sortedElements.length > 0 ? ` affecting ${sortedElements.slice(0, 2).map(([el]) => el.toUpperCase()).join(' and ')} element(s)` : ''}
                </div>
                ` : ''}
                
                <ul style="list-style: none; padding: 0;">
                    <li style="margin-bottom: 0.75rem; padding-left: 1.5rem; position: relative;">
                        <span style="position: absolute; left: 0;">üíÜ</span>
                        <strong>Bodywork Focus:</strong> ${domainScores[0]?.domain.muscle || 'TBD'} / ${domainScores[1]?.domain.muscle || 'TBD'}
                    </li>
                    <li style="margin-bottom: 0.75rem; padding-left: 1.5rem; position: relative;">
                        <span style="position: absolute; left: 0;">‚ö°</span>
                        <strong>Meridian Balancing:</strong> ${domainScores[0]?.domain.meridian || 'TBD'}, ${domainScores[1]?.domain.meridian || 'TBD'}
                    </li>
                    <li style="margin-bottom: 0.75rem; padding-left: 1.5rem; position: relative;">
                        <span style="position: absolute; left: 0;">üå∏</span>
                        <strong>Custom Essence Blend:</strong> ${sortedEssences.slice(0, 5).map(e => e[0]).join(', ') || 'Complete assessment for recommendations'}
                    </li>
                    <li style="margin-bottom: 0.75rem; padding-left: 1.5rem; position: relative;">
                        <span style="position: absolute; left: 0;">üíé</span>
                        <strong>Mineral Support:</strong> ${domainScores.slice(0, 3).map(d => d.domain.mineral).join(', ') || 'TBD'}
                    </li>
                    <li style="padding-left: 1.5rem; position: relative;">
                        <span style="position: absolute; left: 0;">üåô</span>
                        <strong>Seasonal Consideration:</strong> ${domainScores[0]?.domain.season || 'TBD'}
                    </li>
                </ul>
                
                ${sortedEvils.some(([evil]) => evil === 'damp') ? `
                <div style="margin-top: 1rem; padding: 1rem; background: rgba(0,206,209,0.1); border-radius: 8px;">
                    <strong style="color: var(--teal);">üçÑ Recommended:</strong> Vitality Mushroom Elixir for Dampness clearing, detoxification, and cellular support
                </div>
                ` : ''}
            </div>
        </div>
        
        <div class="export-buttons">
            <button class="export-btn print" onclick="window.print()">üñ®Ô∏è Print Protocol</button>
            <button class="export-btn email" onclick="copyProtocol()">üìß Copy for Email</button>
            <button class="export-btn portal" onclick="window.location.href='practitioner-portal.html'">üîê Practitioner Portal</button>
        </div>
    `;
    
    switchTab('results');
    window.scrollTo({ top: 0, behavior: 'smooth' });
}

function copyProtocol() {
    const content = document.getElementById('resultsContent').innerText;
    navigator.clipboard.writeText(content).then(() => {
        alert('Protocol copied to clipboard!');
    });
}
</script>
</body>
</html>
